	    setRotation(this.head, 0.0F, 0.0F, 0.0F);
	    this.headaccessory = new ModelRenderer(this, 44, 0);
	    this.headaccessory.addBox(-3.5F, -6.5F, -3.5F, 7, 7, 7);
	    this.headaccessory.setRotationPoint(0.0F, -6.0F, -1.0F);
	    this.headaccessory.mirror = true;
	    setRotation(this.headaccessory, 0.0F, 0.0F, 0.0F);
	    this.hair1 = new ModelRenderer(this, 52, 14);
	    this.hair1.addBox(-4.0F, -7.5F, -0.5F, 8, 7, 2);
	    this.hair1.setRotationPoint(0.0F, -6.0F, -1.0F);
	    this.hair1.mirror = true;
