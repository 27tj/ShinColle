	    this.hair3.setRotationPoint(0.0F, -6.0F, -1.0F);
	    this.hair3.mirror = true;
	    setRotation(this.hair3, 0.1745329F, 0.0F, 0.0F);
	    this.hair4 = new ModelRenderer(this, 6, 41);
	    this.hair4.addBox(-4.0F, -7.5F, 5.5F, 8, 8, 14);
	    this.hair4.setRotationPoint(0.0F, -6.0F, -1.0F);
	    this.hair4.mirror = true;
	    setRotation(this.hair4, 0.1745329F, 0.0F, 0.0F);
	    this.body = new ModelRenderer(this, 0, 12);
	    this.body.addBox(-2.5F, 0.0F, -1.5F, 5, 10, 3);
	    this.body.setRotationPoint(0.0F, -6.0F, -1.0F);
	    this.body.mirror = true;
	    setRotation(this.body, 0.0872665F, 0.0F, 0.0F);
	    this.chest = new ModelRenderer(this, 0, 25);
	    this.chest.addBox(-2.5F, -1.0F, -1.0F, 5, 2, 2);
	    this.chest.setRotationPoint(0.0F, -4.0F, -2.0F);
	    this.chest.mirror = true;
	    setRotation(this.chest, -2.268928F, 0.0F, 0.0F);
	    this.rightarm = new ModelRenderer(this, 16, 12);
	    this.rightarm.addBox(-2.0F, -1.0F, -1.0F, 2, 10, 2);
	    this.rightarm.setRotationPoint(-2.5F, -4.5F, -1.0F);
	    this.rightarm.mirror = true;
